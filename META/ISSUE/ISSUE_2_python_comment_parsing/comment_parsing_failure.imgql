// This is a comment
// This file demonstrates the Python comment parsing issue

// Base cases with comments
let f0(x) = 1
let f1(x) = 1

// Function declarations with comments
let f2(x) = f1(x) + f0(x)

// Another comment
let f3(x) = f2(x) + f1(x)

// Comments should be ignored
print "comment_test" f3(1) 